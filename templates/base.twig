        </svg>
      </a>

      {% if post.page_tagline %}
        <p class="page-title">{{ post.page_tagline }}</p>
      {% else %}
        {% if post.post_type == "insightsarticles" %}
          <p class="page-title">Insights</p>
        {% elseif post.post_type == "careers"%}
          <p class="page-title">Join Us</p>
        {% elseif post.post_type == "bios" %}
          <p class="page-title">Our People</p>
        {% elseif post.parent == "Capabilities"%}
          <p class="page-title">Capabilities</p>
        {% else %}
          <h1 class="page-title">{{ wp_title }}</h1>
        {% endif %}
      {% endif %}

      <button id="primary-trigger" class="hamburger hamburger--spin" type="button">
        <span class="hamburger-box">
          <span class="hamburger-inner"></span>
        </span>
      </button>
    </div>

    <div id="primary">
      <div>
        <nav id="nav-main" class="nav-main" role="navigation">
          {% include "/templates/components/menu.twig" with {'menu': menu.get_items} %}
        </nav><!-- #nav -->

        <div class="secondary">
          <nav id="nav-contact" class="nav-contact" role="navigation">
            <ul>
              <li>
                <h2 class="level-5 aqua">Email</h2>
                <a href="mailto:{{ options.email }}" class="level-4">{{ options.email }}</a>
              </li>
              <li>
                <h2 class="level-5 aqua">Phone</h2>
                <a href="tel:{{ options.phone|replace({'(': '', ')': '', '–': '', '-': '', ' ': ''}) }}" class="level-4">{{ options.phone }}</a>
              </li>
            </ul>
          </nav>

          <nav id="nav-social" class="nav-social" role="navigation">
            {% include "/templates/components/social.twig" with {'social': social.get_items} %}
          </nav>
        </div>
      </div>
    </div>
  </header>
  {% endblock %}


  {# BODY #}
  <section id="content" role="main" class="content-wrapper">
    {% if title %}<h1>{{title}}</h1>{% endif %}
    {% block content %}
      Sorry, no content
    {% endblock %}
  </section>


  {# FOOTER #}
  <footer>
    <div>
      <ul class="social-icons">
        {% if options.facebook_url %}<li><a href="{{ options.facebook_url }}" class="icon-facebook" target="_blank"></a></li>{% endif %}
        {% if options.glassdoor_url %}<li><a href="{{ options.glassdoor_url }}" class="icon-glassdoor" target="_blank"></a></li>{% endif %}
        {% if options.linkedin_url %}<li><a href="{{ options.linkedin_url }}" class="icon-linkedin" target="_blank"></a></li>{% endif %}
        {% if options.instagram_url %}<li><a href="{{ options.instagram_url }}" class="icon-instagram" target="_blank"></a></li>{% endif %}
      </ul>

      <a href="mailto:{{ options.email }}" class="email">{{ options.email }}</a>
      <a href="tel:{{ options.phone|replace({'(': '', ')': '', '–': '', '-': '', '.': '', ' ': ''}) }}" class="email">{{ options.phone }}</a>
      <small class="level-5">&copy; {{ "now"|date('Y') }} Unify Consulting. All rights reserved.</small>
    </div>
  </footer>
  {{ function('wp_footer') }}

  {% if env == 'development' %}
  {# Provide the compiled JS file. #}
  <script src="{{site.theme.link}}/_ui/compiled/scripts.js"></script>
  {% else %}
  {# Otherwise, provide the compiled and uglified JS file. #}
  <script id="scripts" src="{{site.theme.link}}/_ui/dist/scripts.min.2020519-1589908715339.js"></script>
  {% endif %}
</body>
</html>
