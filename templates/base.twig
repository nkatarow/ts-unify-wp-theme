            <path class="st0" d="M86.2,34.1h3.9v-7.6c0-4.6-3.4-8.1-8-8.1c-4.6,0-8,3.5-8,8.1v7.6H78v-7.6c0-2.2,1.3-4.5,4.1-4.5c2,0,4.1,1.4,4.1,4.5V34.1z"/>
