  {# GLOBAL NAV #}
  {% block header %}
  <header class="header">
    <div class="top-bar container">
      <a class="logo" href="{{site.url}}" rel="home">
        <svg title="{{site.name}}" version="1.1" id="Layer_1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="128.3px" height="48.7px" viewBox="0 0 128.3 48.7" style="enable-background:new 0 0 128.3 48.7;" xml:space="preserve">
