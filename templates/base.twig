<head>
  <meta charset="utf-8" />
  <title>
    {{ wp_title }}
  </title>
  <link rel="home" href="{{ siteUrl }}" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
	<meta name="theme-color" content="#143250">
  <link href="{{ entry.url }}" rel="canonical" />
  <link rel="apple-touch-icon" sizes="180x180" href="{{site.theme.link}}/_ui/img/favicon/apple-icon-180x180.png">
  <link rel="icon" type="image/png" href="{{site.theme.link}}/_ui/img/favicon/favicon-32x32.png" sizes="32x32">
  <link rel="icon" type="image/png" href="{{site.theme.link}}/_ui/img/favicon/favicon-16x16.png" sizes="16x16">
  <link rel="manifest" href="{{site.theme.link}}/_ui/img/favicon/manifest.json">
  <link rel="mask-icon" href="{{site.theme.link}}/_ui/img/favicon/safari-pinned-tab.svg" color="#143250">
  <link rel="pingback" href="{{site.pingback_url}}" />

  {# Test to see if we're in dev mode... #}
  {% if (env == 'development') %}
  {# If so, provide concatonated, header CSS/JS files #}
  <link rel="stylesheet" href="{{site.theme.link}}/_ui/compiled/main.css" type="text/css" media="all">
  {% else %}
  {# If not, provide concatonated, minified/uglified header CSS/JS files #}
  <link id="stylesheet" rel="stylesheet" href="{{site.theme.link}}/_ui/dist/main.min.2020519-1589908715339.css" type="text/css" media="all">
  {% endif %}

  {{function('wp_head')}}

  <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-112611898-1"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag() {dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', 'UA-112611898-1');
  </script>
</head>


